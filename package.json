{
  "name": "sfci",
  "description": "sfci tools",
  "version": "0.0.4",
  "author": "JosÃ© Silas Cardoso Maia Vieira",
  "bin": {
    "sfci": "bin/run"
  },
  "bugs": {
    "url": "https://github.com/https://github.com/silascmv/sfci/issues"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.5",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.60",
    "@types/xml2js": "^0.4.9",
    "chai": "^4.3.4",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.10"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/https://github.com/silascmv/sfci",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfci",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/silascmv/sfci.git"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@oclif/command": "^1.8.6",
    "@oclif/config": "^1.18.1",
    "@oclif/plugin-help": "^3.2.9",
    "app-root-path": "^3.0.0",
    "tslib": "^1.14.1",
    "winston": "^3.3.3",
    "xml2js": "^0.4.23"
  },
  "directories": {
    "test": "test"
  }
}
